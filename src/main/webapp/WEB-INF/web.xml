<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PageAccueil</servlet-name>
  	<display-name>PageAccueil</display-name>
  	<description></description>
  	<servlet-class>servlets.PageAccueil</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Inscription</servlet-name>
  	<display-name>Inscription</display-name>
  	<description></description>
  	<servlet-class>servlets.Inscription</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Deconnexion</servlet-name>
  	<display-name>Deconnexion</display-name>
  	<description></description>
  	<servlet-class>servlets.Deconnexion</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PageAccueil</servlet-name>
  	<url-pattern>/PageAccueil</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Inscription</servlet-name>
  	<url-pattern>/Inscription</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deconnexion</servlet-name>
  	<url-pattern>/Deconnexion</url-pattern>
  </servlet-mapping>
</web-app>
